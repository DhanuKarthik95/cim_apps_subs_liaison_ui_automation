{
  "plugins": [
    "wdio"
  ],
  "extends": [
    "eslint:recommended",
    "airbnb-base",
    "prettier",
    "plugin:wdio/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourcetype": "module"
  },
  "rules": {
    "indent": 2,
    "no-console": 0,
    "no-shadow": 0,
    "strict": 0,
    "quotes": 0,
    "max-len": 0,
    "object-curly-newline": 0,
    "import/no-named-as-default-member": 0,
    "class-methods-use-this": 0,
    "no-unused-expressions": 0,
    "global-require": 0,
    "import/named": 0,
    "import/no-cycle": 0,
    "consistent-return": 0,
    "no-plusplus": 0
    /* eslint-disable import/no-mutable-exports */
    /* eslint-disable eqeqeq */
  }
}